{
    "name": "st-open-source",
    "version": "0.1.0",
    "description": "ST Open Source - Data synchronization bridge for business systems",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc && tsc-alias",
        "start": "node dist/index.js",
        "test": "jest",
        "test:unit": "jest --config jest.unit.config.js --runInBand",
        "test:integration": "jest --config jest.integration.config.js",
        "test:all": "jest",
        "test:watch": "jest --watch",
        "test:watch:unit": "jest --config jest.unit.config.js --watch",
        "test:watch:integration": "jest --config jest.integration.config.js --watch",
        "test:coverage": "jest --coverage",
        "test:coverage:unit": "jest --config jest.unit.config.js --coverage",
        "test:coverage:integration": "jest --config jest.integration.config.js --coverage",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "format": "biome format .",
        "format:fix": "biome format --write .",
        "check": "biome check .",
        "security:audit": "npm audit",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "db:seed": "tsx prisma/seed.ts",
        "docker:build": "docker build -t st-open-source .",
        "docker:build:tag": "docker build --build-arg DOCKER_TAG=${npm_package_version} --build-arg VERSION=${npm_package_version} -t st-open-source:${npm_package_version} .",
        "docker:build:latest": "docker build --build-arg DOCKER_TAG=latest --build-arg VERSION=${npm_package_version} -t st-open-source:latest .",
        "docker:run": "docker run -p 3000:3000 st-open-source",
        "docker:run:tag": "docker run -p 3000:3000 st-open-source:${npm_package_version}",
        "docker:compose": "docker-compose up -d",
        "docs:generate": "typedoc --out docs src",
        "clean": "rm -rf dist node_modules/.cache",
        "prebuild": "npm run db:generate"
    },
    "keywords": [
        "data-sync",
        "business-integration",
        "api",
        "postgresql",
        "prisma",
        "typescript",
        "express"
    ],
    "author": "ST Open Source Contributors",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^6.11.1",
        "axios": "^1.11.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.2",
        "biome": "^0.3.3",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^17.2.0",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.1",
        "express-validator": "^7.2.1",
        "geoip-lite": "^1.4.10",
        "helmet": "^8.1.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "node-cron": "^4.2.1",
        "speakeasy": "^2.0.0",
        "ua-parser-js": "^2.0.4",
        "winston": "^3.17.0",
        "winston-loki": "^6.1.3",
        "zod": "^4.0.0"
    },
    "devDependencies": {
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/geoip-lite": "^1.4.4",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.0.12",
        "@types/node-cron": "^3.0.11",
        "@types/speakeasy": "^2.0.10",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "jest": "^30.0.4",
        "prisma": "^6.11.1",
        "supertest": "^7.1.3",
        "ts-jest": "^29.4.0",
        "tsc-alias": "^1.8.16",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0",
        "pnpm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stratagems-com/stratagems-tools-open-source.git"
    },
    "bugs": {
        "url": "https://github.com/stratagems-com/stratagems-tools-open-source/issues"
    },
    "homepage": "https://github.com/stratagems-com/stratagems-tools-open-source#readme"
}